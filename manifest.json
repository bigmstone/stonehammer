{
  "manifest_version": 3,
  "name": "StoneHammer",
  "description": "Collection of random things for Matt Stone.",
  "version": "2025.01.02",
  "permissions": [
    "tabs",
    "scripting",
    "declarativeNetRequest"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "block_sites",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "host_permissions": ["<all_urls>"],
  "key": ""
}
